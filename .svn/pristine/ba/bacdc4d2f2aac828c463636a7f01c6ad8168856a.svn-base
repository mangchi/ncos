package mil.ln.ncos.client.udp.scheduler;

import org.quartz.JobExecutionContext;
import org.quartz.JobExecutionException;
import org.springframework.scheduling.quartz.QuartzJobBean;

import lombok.RequiredArgsConstructor;
import mil.ln.ncos.client.service.UdpOutboundService;

@RequiredArgsConstructor
public class OutboundThreatJob extends QuartzJobBean {
	
	private final UdpOutboundService udpOutboundService;

	@Override
	protected void executeInternal(JobExecutionContext context) throws JobExecutionException {
		this.udpOutboundService.send(2);
		
	}


}
