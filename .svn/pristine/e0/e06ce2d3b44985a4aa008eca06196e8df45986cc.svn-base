@charset "utf-8";/* CSS Document */
body{color:#FFF}
a{color:#fff}
a:active, /* a:hover{color:#ed1c24} */
a.basic{color:#fff}
a.basic:active, a.basic:hover{color:#ea817b}
.app{ background:#130111}
.navbar{position:fixed;display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;padding:0px;top:0;left:0;right:0;height:100px;width:100%;border-bottom:10px solid #130111;z-index:99999}
.navbar-nav{display:-ms-flexbox;display:flex;-ms-flex-direction:row;flex-direction:row;flex-flow:row nowrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;list-style:none;padding:0px 20px;width:100%;height:60px;position:relative;z-index:100}
.navbar-nav .nav-link{vertical-align:middle}
.navbar-nav .nav-link .icon-holder{border-radius:6px;display:inline-block;font-size:22px;height:34px;left:0;line-height:34px;margin-right:0px;position:relative;text-align:center;-webkit-transition:all .3s ease;-o-transition:all .3s ease;transition:all .3s ease;width:35px}

.navbar-nav h1{display:inline-block;margin-left:20px;font-size:22px;font-weight:bold;vertical-align:middle}
.navbar-nav h1 > strong{display:inline-block;vertical-align:middle}
.navbar-location{display:-ms-flexbox;display:flex;-ms-flex-direction:row;flex-direction:row;flex-flow:row nowrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;list-style:none;padding:0px 20px;width:100%;height:30px;background:#383e4e}
.navbar-location .navigation{display:inline-flex;align-content:flex-start;font-size:14px;font-weight:bold}
.navbar-location .navigation li{display:inline-block;padding:0 15px;line-height:20px;position:relative}
.navbar-location .navigation li:first-child{padding-left:5px}
.navbar-location .navigation li:first-child:after{display:none}
.navbar-location .navigation li:last-child{padding-right:2px}
.navbar-location .navigation li:after{content:' >';position:absolute;font-size:10px;color:#FFF;top:0px;left:-3px;width:10px;height:10px}

.footbar{position:fixed;display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;padding:0;left:0;right:0;bottom:0;height:70px;border-top:10px solid #130111}
.footbar-nav{display:-ms-flexbox;display:flex;-ms-flex-direction:row;flex-direction:row;flex-flow:row nowrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;padding:0px 20px;margin-bottom:0;list-style:none;width:100%}
.footbar-nav .foot-link{padding-right:10px;padding-left:10px}

.bg-light{background-color:#151a2d!important}
a.bg-light:focus,a.bg-light:hover,button.bg-light:focus,button.bg-light:hover{background-color:#cfd9db!important}
.container-wide{width:100%;height:100%;overflow:hidden;padding-left:70px;-webkit-transition:all .2s ease;-o-transition:all .2s ease;transition:all .2s ease;padding-top:100px;padding-bottom:70px}
.container{width:100%;height:100%;padding:0 5px;background:#130111;overflow:auto;margin:0 auto}

.container .sub-title-box{display:flex;flex-direction:row;flex-flow:row nowrap;justify-content:space-between;padding:10px 5px;width:100%}
.container .sub-title-box .title{display:inline-block;font-size:18px;font-weight:700;line-height:20px}

.grid-row{display:flex;width:100%;align-items:stretch;flex-direction:row;flex-wrap:nowrap;align-content:flex-start;justify-content:flex-start;min-width:1800px}
.grid-row:first-child .item-box{padding-top:0px}
.grid-row:last-child .item-box{padding-bottom:0px}
.grid-row .item-box{display:inline-flex;padding:5px}
.grid-row .item-box .inner{display:block;width:100%;background:#151a2d;/* border:1px solid #e5e5e5;border-radius:5px;box-shadow:0 0 5px rgba(0,0,0,0.05)*/}
.grid-row .item-box .inner .item-title{display:block;padding:10px;font-size:15px;font-weight:700;text-align:left;border-bottom:1px solid #090c17;position:relative}
.grid-row .item-box .inner .item-title > strong{display:inline-block;vertical-align:middle}
.grid-row .item-box .inner .item-title .setting{ position:absolute;top:9px;right:0px;overflow:hidden;text-align:left;text-indent:-999999px}
.grid-row .item-box .inner .items{position:relative;display:block;padding:10px;overflow:auto}
.grid-row .item-box .inner .items.dashtop{padding:10px 10px;}
.grid-row .item-box .inner .items.mh{min-height:200px}
.grid-row .item-box .inner .items .imgWrap{display:block}
.grid-row .item-box .inner .items .imgWrap img{display:block;width:100%}
.grid-row .item-box .inner .items.minH{min-height:200px}


/*.Popup-container .grid-row .item-box .inner{background:#fafafa;border:1px solid #e5e5e5}
.Popup-container .grid-row .item-box .inner .item-title{background:#f2f2f2}*/

.col-1{width:8.33%}
.col-2{width:16.66%}
.col-20{width:20%}
.col-22{width:22.55%}
.col-3{width:25%}
.col-4{width:33.33%}
.col-5{width:41.66%}
.col-6{width:50%}
.col-64{width:53.2%}
.col-7{width:58.33%}
.col-8{width:66.66%}
.col-9{width:75%}
.col-10{width:83.33%}
.col-11{width:91.66%}
.col-86{width:86.50%}
.col-12{width:100%}

a{text-decoration:none}
.sidebar{bottom:70px;/*overflow:hidden;*/position:fixed;top:100px;-webkit-transition:all .2s ease;-o-transition:all .2s ease;transition:all .2s ease;width:70px;background-color:#151a2d;color:#fff;z-index:9999;/*box-shadow:5px 0px 5px 0px #130111;*/ padding-top:20px}
.sidebar ul{list-style-type:none}
.sidebar-inner{position:relative;height:100%}

.sidebar-menu{height:calc(100vh - 65px);list-style:none;margin:0;/*overflow:auto;*/padding:0;position:relative}
.sidebar-menu .sidebar-link.actived:before{background:#0f9aee;border-radius:50%;content:"";display:block;height:8px;left:-4px;position:absolute;top:calc(50% - 4px);width:8px}
.sidebar-menu > li{position:relative}
.sidebar-menu > li > a{color:#fff;-webkit-transition:all .3s ease;-o-transition:all .3s ease;transition:all .3s ease}
.sidebar-menu > li > a:focus, .sidebar-menu > li > a:hover{color:#fff;text-decoration:none;background-color:#383e4e}
.sidebar-menu > li > a{display:block;font-size:15px;padding:15px 22px;position:relative;white-space:nowrap}
.sidebar-menu > li > a > .tooltip{display:none;position:absolute;left:60px;top:10px;padding:10px 20px;background:#fff;color:#000;border-radius:5px}
.sidebar-menu > li > a > .tooltip::before{content:" ";position:absolute;top:12px;left:-5px;margin-left:-5px;border-width:5px;border-style:solid;border-color:transparent #fff transparent transparent}
.sidebar-menu > li > a:hover > .tooltip{display:inline-block}

.float-left{float:left !important}
.float-right{float:right !important}
.float-none{float:none !important}

/* Button */
.btnWrap{display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;padding:5px 0;width:100%}
.btn{display:inline-block;text-align:center;white-space:nowrap;word-wrap:normal;word-break:normal}

/* Button Size */
.btn.ssmall{padding:0 9px;font-size:11px;line-height:16px;border-radius:5px;font-weight:700}
.btn.small1{padding:0 12px; margin:0px; font-size:13px;line-height:22px;border-radius:5px;font-weight:700}
.btn.small1 > strong{display:inline-block;vertical-align:middle;line-height:22px}
.btn.small{padding:0 12px;font-size:12px;line-height:22px;border-radius:5px;font-weight:700}
.btn.small > strong{display:inline-block;vertical-align:middle;line-height:22px}
.btn.medium{padding:0 15px;font-size:13px;line-height:32px;border-radius:5px;font-weight:700}
.btn.medium > strong{display:inline-block;vertical-align:middle;line-height:32px}
.btn.large{padding:0 18px;font-size:14px;line-height:40px;border-radius:5px;font-weight:700}
.btn.xlarge{padding:0 21px;font-size:15px;line-height:48px;border-radius:5px;font-weight:700}

.btn.board{padding:0 15px;line-height:28px;border-radius:2px}
.btn.brdronone{border-radius:0 !important}

/* Button Color */
.btn.orange{background:#ff7100;border:1px solid #f65a05;color:white}
.btn.blue{background:#51b6dd;border:1px solid #40a1c7;color:white}
.btn.green{background:#7fb50f;color:white}
.btn.white{background:#FFF;border:1px solid #b8b8b8;color:#333;box-shadow:0px 1px 3px 0px lightgray}
.btn.gray{background:#aba79e;border:1px solid #aba79d;color:white}
.btn.brown{background:#8f805f;border:1px solid #8f805f;color:white}
.btn.lightgray{background:#eee;border:1px solid #d7d7d7;color:#303030}
.btn.white:hover, .btn.white:focus{background-color:#fbfbfb;border:1px solid #a6a6a6}
.btn.darkblue{background:#4d525e;border:1px solid #4d525f;color:#FFF}
.btn.purple{background:#8065a5;border:1px solid #6a4f8f;color:#FFF}
.btn.blue2{background:#4094f0;border:1px solid #3082dd;color:white}
.btn.white2{background:#FFF;border:1px solid #4d4d4d;color:#333}
.btn.red{background:#cf2a27;border:1px solid #cf2a27;color:#FFF}

/* .btn-example{position:absolute;left:50%;top:50%;margin:-17px 0 0 -42px} */
.pop-layer-rb{width:300px;display:none;position:absolute;right:10px;bottom:10px;background:#242b3d;border:1px solid #333333 !important;border-radius:7px !important;z-index:99999999;box-sizing:border-box !important;/*overflow:hidden;*/box-shadow:0px 0px 10px rgba(0,0,0,0.1)}
.dim-layer .pop-layer-rb{display:block}

/* .btn-example{position:absolute;left:50%;top:50%;margin:-17px 0 0 -42px} */
.pop-layer{display:none;position:absolute;top:50%;left:50%;background:#242b3d;border:1px solid #333333 !important;border-radius:7px !important;z-index:99999999;box-sizing:border-box !important;/*overflow:hidden;*/box-shadow:0px 0px 10px rgba(0,0,0,0.1)}
.pop-layer.chattext{position:relative;padding-bottom:80px !important;display:block;width:100%}
.dim-layer{display:none;position:absolute;align-items:center;justify-content:center;top:0;left:0;width:100%;height:100%;z-index:9999999}
.dim-layer .dimBg{position:absolute;top:0;left:0;width:100%;height:100%;background:#000;opacity:0.5;filter:alpha(opacity=50)}
.dim-layer .pop-layer{display:block}

.pop-layer.ssssmall{width:130px;min-height:140px;/*margin:-100px 0 0 -150px*/}
.pop-layer.sssmall{width:230px;min-height:200px;/*margin:-100px 0 0 -150px*/}
.pop-layer.ssmall{width:300px;min-height:200px;/*margin:-100px 0 0 -150px*/}
.pop-layer.small{width:400px;min-height:230px;/*margin:-150px 0 0 -200px*/}
.pop-layer.h-small{width:400px;min-height:340px;/*margin:-270px 0 0 -200px*/}
.pop-layer.h2-small{width:400px;min-height:250px;/*margin:-225px 0 0 -200px*/}
.pop-layer.medium{width:500px;min-height:300px;/*margin:-200px 0 0 -250px*/}
.pop-layer.large{width:600px;min-height:400px;/*margin:-250px 0 0 -300px*/}
.pop-layer.xlarge{width:800px;min-height:500px;/*margin:-320px 0 0 -400px*/}
.pop-layer.xxlarge{width:1000px;min-height:600px;/*margin:-320px 0 0 -400px*/}
.pop-layer.xxxlarge{width:1200px;min-height:600px;/*margin:-320px 0 0 -400px*/}
.pop-layer.xxxxlarge{width:1600px;min-height:600px;/*margin:-320px 0 0 -400px*/}

.Popup-title{height:50px !important;padding:0px 20px !important;border-bottom:1px solid #151a2d;position:relative;background:#151a2d}
.Popup-title .icon-close{position:absolute;top:18px !important;right:20px !important;margin:0 !important;display:inline-block;width:17px;height:17px;background:url(../img/icon/dark/btn_modal_close.png) no-repeat 0 0;border:none;overflow:hidden;text-align:left;text-indent:-9999px}
.Popup-title h3{font-size:16px;line-height:50px;font-weight:700;color:#FFF;display:inline-block}
.Popup-title h3 em{color:blue;text-decoration:underline;font-style:normal}
.Popup-title .version{display:inline-block;margin-left:20px;background:transparent;border:none;padding:10px;transform:rotate(-0.03deg)}
.Popup-container{/*overflow:auto !important;*/padding:20px;color:#FFF;/*max-height:540px !important*/}

.Popup-contents{min-height:42px;/* overflow:hidden;*/}
.Popup-contents2{min-height:42px;/*overflow:hidden;*/ padding:20px;text-align:center;line-height:1.5}
.Popup-contents2.red{background:red;color:#FFF}
.Popup-contents2.orange{background:orange;color:#FFF}
.Popup-contents .inner-text{min-height:42px;line-height:1.5;padding-bottom:20px;word-break:keep-all}
.Popup-contents .inner-text1{min-height:30px;line-height:1.5;padding-bottom:5px;word-break:keep-all;font-weight:bold}
.Popup-contents .inner-text .agree_text{padding:0px;font-size:13px;line-height:2}
.Popup-contents .inner-text a{color:#1552CD;text-decoration:underline}
.Popup-tenantArea .Button + .Button{margin-left:0}
.Popup-contents .Button-large a[class^="button-"]{display:table;width:453px;height:70px;margin-bottom:5px}
.Popup-contents .Button-large .inner{display:table-cell;vertical-align:middle}
.Popup-contents .Button-large .tit{display:block;margin-bottom:5px;font-size:13px;text-align:center}
.Popup-contents .Button-large .note{display:block;text-align:center}
.Popup-contents .Button-large .Button-solid{background:#5160b5;color:#fff}
.Popup-contents .Button-large .Button-solid:hover{background:#4d5bac}
.Popup-contents .Button-large .Button-solid .note{opacity:0.7}
.Popup-contents .Button-large .Button-line{background:#fff;border:1px solid #c8cacc}
.Popup-contents .Button-large .Button-line:hover{background:#f4f4f5}
.Popup-contents .Button-large .Button-line .note{color:#919699}
.Popup-contents .GridStateBar{padding-top:4px !important;padding-bottom:0;height:25px;line-height:14.7px;margin-bottom:0}
.Popup-contents .Popup-table{margin-bottom:20px}
.Popup-contents .Popup-table th{vertical-align:top;padding-top:8px;text-align:left}
.Popup-contents .Popup-table td{padding-bottom:20px}
.Popup-contents .Popup-table td span{padding-top:6px;display:inline-block}
.Popup-contents .Popup-table td .SearchBox-searchArea{padding-top:0 !important}
.Popup-contents .Popup-table.ty01{margin-bottom:0}
.Popup-contents .Popup-table.ty01 td{padding-bottom:10px}
.Popup-contents .messageContent{ min-height: 50px; text-align: center} 
.success{color:#ffffff; }
.fail{ color:#ff0000;}


.Popup-footer{padding:10px 20px !important;border-top:1px solid #151a2d;position:relative;background:#242b3d;text-align:right}
.Popup-footer.ac{text-align: center !important}

.Popup-buttonArea{position:absolute;left:0;bottom:0;width:100%}
.Popup-buttonArea .aichatbox{display:flex;width:100%;padding:15px 20px;border-top:3px solid #e5e5e5;height:80px;align-items:center;background:#f7f7f7}
.Popup-buttonArea .aichatbox .userText{display:inline-flex;height:100%;outline:none;/*overflow:auto;*/font-size:14px;font-weight:700;line-height:18px;width:100%;border:none;background:transparent;padding:0px;resize:none}
.Popup-buttonArea .aichatbox .sendBtn{display:inline-flex;flex:0 0 62px;align-items:center;justify-content:center;height:44px;line-height:44px;font-weight:700;transform:rotate(-0.03deg);text-align:center;background:#364b7a;border:1px solid #364b7a;color:#FFF;border-radius:3px}

.tablewrap table{border:1px #130111 solid;width:100%;border-collapse:collapse;overflow:hidden}
.tablewrap thead tr{font-weight:bold;color:#fff;background:#151a2d}
.tablewrap tbody tr:nth-child(even){background-color:#151a2d}
.tablewrap tbody tr:nth-child(odd){background-color:#242b3d}
.tablewrap tbody tr.trSelect{background-color:#003366}
.tablewrap tbody tr:hover{background-color:#5A78AF}
.tablewrap th{font-weight:bold;text-align:center;border-left:1px solid rgba(0,0,0,1);border-right:1px solid rgba(0,0,0,1);border-bottom:1px solid rgba(0,0,0,1);padding:14px 8px;vertical-align:middle;white-space:nowrap}
.tablewrap td{text-align:center;border-left:1px solid rgba(0,0,0,1);border-right:1px solid rgba(0,0,0,1);padding:8px;vertical-align:middle;white-space:nowrap;height:43px}
.tablewrap td .threat-high{display:inline-block;text-align:center;min-width:26px;height:26px;padding:0 5px;color:#FFF;background:#ff0000;border-radius:13px;line-height:26px;font-weight:bold}
.tablewrap td .threat-middle{display:inline-block;text-align:center;min-width:26px;height:26px;padding:0 5px;color:#FFF;background:#ff8000;border-radius:13px;line-height:26px;font-weight:bold}
.tablewrap td .threat-low{display:inline-block;text-align:center;min-width:26px;height:26px;padding:0 5px;color:#000;background:#ffcc00;border-radius:13px;line-height:26px;font-weight:bold}
.tablewrap a{color:#FFF}

.page_wrap{text-align:center;font-size:0;padding:20px 0}
.page_nation{display:inline-block}
.page_nation .none{display:none}
.page_nation a{display:block;margin:0 3px;float:left;border:1px solid #130111;width:32px;height:32px;line-height:30px;text-align:center;background-color:#151a2d;font-size:14px;color:#FFF;text-decoration:none}
.page_nation .arrow{border:1px solid #130111}
.page_nation .pprev{background:#151a2d url('../img/page_pprev.png') no-repeat center center;margin-left:0}
.page_nation .prev{background:#151a2d url('../img/page_prev.png') no-repeat center center;margin-right:7px}
.page_nation .next{background:#151a2d url('../img/page_next.png') no-repeat center center;margin-left:7px}
.page_nation .nnext{background:#151a2d url('../img/page_nnext.png') no-repeat center center;margin-right:0}
.page_nation a.active{background-color:#242b3d;color:#fff;order:1px solid #130111}

.schwrap{display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap;-ms-flex-align:center;align-items:center;-ms-flex-pack:justify;justify-content:space-between;padding:0;width:100%}
.schwrap ul{/*display:inline-block*/}
.schwrap ul li{display:inline-block;padding-right:20px;text-align:right}
.schwrap ul li label{display:block;flex-direction:column;flex-flow:column}
.schwrap ul li .selectWrap{display:block;flex-direction:column;flex-flow:column;position:relative}
.schwrap ul li label .form-control,.schwrap ul li label .form-select{min-width:160px}
.form-control{display:inline-block;height:calc(1.5em + 0.75rem + 2px);padding:0.375rem 0.75rem;margin:5px 0;line-height:1.5;color:#ccc;background-color:rgba(255,255,255,0.1);background-clip:padding-box;border:1px solid #6c707b;border-radius:0.375rem;transition:border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out}
.form-select{display:inline-block;padding:0.375rem;line-height:1.5;color:#ccc;background-color:rgba(255,255,255,0.1);border:1px solid #6c707b;border-radius:0.375rem;margin:5px 0}

.dataNum{font-size:80px;font-weight:bold;color:orange;padding:10px 0 20px 0;margin-top: 25px}

.assetNormal{font-size:60px;font-weight:bold;color:green;padding:10px 0 20px 0;margin-top: 25px;}

.defenseStep{display:block;text-align:center;padding-top:50px}
.defenseStep ul{display:inline-block}
.defenseStep ul li{display:inline-block;float:left;padding:2px}
.defenseStep ul li > span{display:inline-block;padding:8px 8px 10px 8px;font-size:18px;font-weight:bold;color:#000;background:#FFA500;border:2px solid #000;border-radius:3px}
.defenseStep ul li > span.step{background:#FF0004;color:#FFF}

.eventRatingPop{display:block;text-align:center;padding:15px}
.eventRatingPop ul{display:inline-block}
.eventRatingPop ul li{display:inline-block;float:left;padding:2px}
.eventRatingPop ul li > span{display:inline-block;padding:3px 5px;font-size:28px;font-weight:bold}
.eventRatingPop ul li > strong{display:inline-block;padding:3px 5px;font-size:28px;font-weight:bold}
.eventRatingPop ul li.high{color:red}
.eventRatingPop ul li.middle{color:orange}
.eventRatingPop ul li.low{color:yellow}

.eventNumPop{display:block;text-align:center;padding:15px}
.eventNumPop ul{display:inline-block}
.eventNumPop ul li{display:inline-block;float:left;padding:2px;color:#FFF}
.eventNumPop ul li > span{display:inline-block;padding:3px 5px;font-size:28px;font-weight:normal}
.eventNumPop ul li > strong{display:inline-block;padding:3px 5px;font-size:28px;font-weight:bold}

.threatImportance{display:block;text-align:center;padding-top:0px}
.threatImportance ul{display:inline-block}
.threatImportance ul li{display:flex;align-items: center; justify-content: space-between; width: 100%; flex-wrap:wrap;padding:2px}
.threatImportance ul li > span{display:block;padding:6px 20px;font-size:32px;font-weight:bold}
.threatImportance ul li > strong{display:block;padding:6px 20px;font-size:42px;font-weight:bold}
.threatImportance ul li.high{color:red}
.threatImportance ul li.middle{color:orange}
.threatImportance ul li.low{color:yellow}

.eventRating{display:block;text-align:center;padding:20px 15px 20px 15px}
.eventRating ul{display:inline-block}
.eventRating ul li{display:flex;padding:2px;align-items: center; justify-content: space-between; width: 100%; flex-wrap:wrap;}
.eventRating ul li > span{display:block;padding:6px 20px;font-size:32px;font-weight:bold}
.eventRating ul li > strong{display:block;padding:6px 20px;font-size:42px;font-weight:bold}
.eventRating ul li.high{color:red}
.eventRating ul li.middle{color:orange}
.eventRating ul li.low{color:yellow}

.eventNum{display:block;text-align:center;padding:10px 15px 25px 15px; margin-top: 20px}
.eventNum ul{display:inline-block}
.eventNum ul li{display:inline-block;float:left;padding:2px;color:#FFF}
.eventNum ul li > span{display:block;padding:6px 20px;font-size:40px;font-weight:normal}
