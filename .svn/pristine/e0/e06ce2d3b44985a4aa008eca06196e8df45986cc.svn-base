.select ul li,
.select .selected .selected-value{white-space:nowrap;overflow:hidden;text-overflow:ellipsis;padding-left:5px}
.select ul{width:160px;display:none;position:absolute;left:1px;top:40px;background:#2d3242;border:1px solid #6c707b;color:#FFF;border-radius:0.375rem;margin:1px 0 0 -1px;cursor:pointer;padding:5px;max-height: 190px;overflow: auto}
.select.active ul{display:block;z-index:999999}
.select ul li{display:block !important;padding:8px 5px;text-align:left !important;color:#ffffff;background-color:#2d3242}
.select ul li:hover{background-color:#1e90ff}
.multiselect .selected{min-width:160px;font-weight:bold;display:inline-block;padding:0.315rem;line-height:1.5;background:#2d3242;border:1px solid #6c707b;color:#FFF;border-radius:0.375rem}
.multiselect .selected .selected-value{text-align:left;padding-left:5px}
