.selectBox .selected .selected-value{width:160px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;padding-left:5px}
.overSelect{position:absolute;left:0;right:0;top:0;bottom:0;z-index:200}
.checkboxes{position:absolute;left:0;top:36px;display:none;width:100%;text-align:left;border:1px solid #6c707b;color:#FFF;z-index:100;border-radius:0.375rem;max-height: 190px;overflow: auto}
