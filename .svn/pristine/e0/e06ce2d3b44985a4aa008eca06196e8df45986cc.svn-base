.assetWrap > ul > li > label.error{padding-bottom:20px; position: relative}
.assetWrap > ul > li > label.error .message{display: inline-block; position: absolute;top:36px; right: 0; width: 160px; height: 20px; line-height: 20px;color: #ff6600; font-size: 11px; font-style: normal}
.assetWrap > ul > li > label.error input{border:1px solid #ff6600}
.assetWrap > ul > li > label::after{content:"";clear:both;;display:block}
.assetWrap > ul > li > label > span{display:inline-block;width:98px;float:left;line-height:40px;}
.assetWrap.tfpositive > ul > li > label > span{display:inline-block;width:105px;float:left;line-height:40px;}
.assetWrap > ul > li > label > span.required{color:#ff6600}
.assetWrap > ul > li > label > select,.assetWrap > ul > li > label > input{display:block;margin-left:98px;width:160px}
.assetWrap.tfpositive > ul > li > label > textarea{display:block;margin-top:10px; width: 440px; height: 100px; border: 1px solid #c2c2c2; background: #fafafa; padding: 10px; line-height: 1.5;border-radius: 5px;}

.settingWrap{display:block}
