.treejs{-webkit-box-sizing:border-box;box-sizing:border-box;font-size:14px}
.treejs *:after,
.treejs *:before{-webkit-box-sizing:border-box;box-sizing:border-box}
.treejs > .treejs-node{padding-left:0}
.treejs .treejs-nodes{list-style:none;padding-left:20px;overflow:hidden;-webkit-transition:height 150ms ease-out, opacity 150ms ease-out;-o-transition:height 150ms ease-out, opacity 150ms ease-out;transition:height 150ms ease-out, opacity 150ms ease-out}
.treejs > .treejs-node{padding-left:0}
.treejs .treejs-node{cursor:pointer;overflow:hidden;padding:5px 0}
.treejs .treejs-node.treejs-placeholder{padding-left:20px}
.treejs .treejs-node.treejs-placeholder:hover{background-color:#5A78AF}
