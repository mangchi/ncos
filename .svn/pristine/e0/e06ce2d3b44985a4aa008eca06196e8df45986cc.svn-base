.select{min-width:160px;position:relative;display:inline-block;vertical-align:middle}
.select .selected{position:relative;width:100%;font-weight:bold;display:inline-block;padding:0.315rem;line-height:1.5;background:#2d3242;border:1px solid #6c707b;color:#FFF;border-radius:0.375rem;margin:5px 0}
.select .selected .selected-value{width:100%;text-align:left}
